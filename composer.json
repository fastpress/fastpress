{
   "name":"fastpress/fastpress",
   "description":"Fast, Simple framework",
   "keywords":[
      "php blog",
      "blog app",
      "fastpress"
   ],
   "homepage":"https://github.com/fastpress/fastpress",
   "license":"MIT",
   "authors":[
      {
         "name":"Simon Daniel",
         "email":"bountyhub@gmail.com",
         "role":"Creator / Developer"
      }
   ],
   "require":{
      "php":"^8.0",
      "fastpress/framework":"v0.2.*"
   },
   "config":{
      "optimize-autoloader":true
   },
   "require-dev":{
      "phpunit/phpunit":"^5.0"
   },
   "autoload":{
      "psr-4":{
         "App\\Model\\":"src/app/model/",
         "App\\Controller\\":"src/app/controller/"
      }
   }
}
