{
   "name":"fastpress/fastpress",
   "description":"fast and simple php blogging application",
   "keywords":[
      "php blog",
      "blog app",
      "fastpress"
   ],
   "homepage":"https://github.com/fastpress/fastpress",
   "license":"MIT",
   "authors":[
      {
         "name":"Simon Daniel",
         "email":"eritr3a@gmail.com",
         "role":"Creator / Developer"
      }
   ],
   "require":{
      "php":">=5.6",
      "fastpress/framework":"0.1.*"
   },
   "config":{
      "optimize-autoloader":true
   },
   "require-dev":{
      "phpunit/phpunit":"^5.0"
   },
   "autoload":{
      "psr-4":{
         "App\\Model\\":"src/app/model/",
         "App\\Controller\\":"src/app/controller/"
      }
   }
}
